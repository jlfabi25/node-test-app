{
  "name": "node-test-app",
  "version": "1.0.0",
  "description": "node test app used for testing CD integration",
  "main": "app/server.js",
  "scripts": {
    "start": "node ./app/server.js",
    "watch": "nodemon ./app/server.js",
    "clean:cover": "rimraf coverage",
    "cover": "npm run clean:cover && istanbul cover node_modules/mocha/bin/_mocha -- --recursive ./test/",
    "lint:js": "standard",
    "test:js": "mocha --recursive ./test",
    "test": "npm run test:js"
  },
  "standard": {
    "globals": [
      "beforeEach",
      "afterEach",
      "describe",
      "it"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlfabi/IT-697/tree/master/code"
  },
  "author": "jfabi",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "node-mocks-http": "^1.5.2",
    "rimraf": "^2.5.4",
    "should": "^11.1.0",
    "sinon": "^1.17.4",
    "standard": "^7.1.2",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "acl": "^0.4.9",
    "akamai": "^0.10.1",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.2",
    "dot-env": "0.0.1",
    "express": "^4.14.0",
    "guid": "0.0.12",
    "jade": "~1.11.0",
    "lodash": "^4.14.2",
    "mongodb": "^2.2.9",
    "monk": "^3.1.2",
    "morgan": "^1.7.0",
    "node-uuid": "^1.4.7",
    "redis": "^2.6.2",
    "request": "^2.74.0"
  }
}
